{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "MariaDB client plugin schema",
  "additionalProperties": false,
  "required": [
    "cnf"
  ],
  "properties": {
    "cnf": {
      "type": "string",
      "description": "The .cnf path to use"
    },
    "pool_size": {
      "type": "number",
      "default": 1,
      "description": "The connection pool size"
    }
  }
}
